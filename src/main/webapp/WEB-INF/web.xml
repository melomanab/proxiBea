<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ServletLogin</servlet-name>
  	<display-name>ServletLogin</display-name>
  	<description></description>
  	<servlet-class>presentation.ServletLogin</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletListeClients</servlet-name>
  	<display-name>ServletListeClients</display-name>
  	<description></description>
  	<servlet-class>presentation.ServletListeClients</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletEdition</servlet-name>
  	<display-name>ServletEdition</display-name>
  	<description></description>
  	<servlet-class>presentation.ServletEdition</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletComptes</servlet-name>
  	<display-name>ServletComptes</display-name>
  	<description></description>
  	<servlet-class>presentation.ServletComptes</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ServletLogin</servlet-name>
  	<url-pattern>/ServletLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletListeClients</servlet-name>
  	<url-pattern>/ServletListeClients</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletEdition</servlet-name>
  	<url-pattern>/ServletEdition</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletComptes</servlet-name>
